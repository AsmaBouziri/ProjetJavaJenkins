FROM openjdk:11

# Installe Maven
RUN apt-get update && \
    apt-get install -y maven && \
    rm -rf /var/lib/apt/lists/*

EXPOSE 8080

COPY pom.xml ./
RUN mvn dependency:copy-dependencies

COPY target/test-jenkins-1.0-SNAPSHOT.jar /app/projetjavajenkins.jar

ENTRYPOINT ["java", "-jar", "/app/projetjavajenkins.jar"]
